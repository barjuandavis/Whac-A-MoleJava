      </ImageView>
      <Label layoutX="200.0" layoutY="92.0" prefHeight="124.0" prefWidth="241.0" text="You Win!!" textFill="#d9ff00">
         <font>
            <Font name="Coffee At Midnight Demo" size="65.0" />
         </font>
      </Label>
      <Label fx:id="lastScore" layoutX="224.0" layoutY="221.0" text="Score : ">
         <font>
            <Font name="Coffee At Midnight Demo" size="32.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="377.0" layoutY="322.0" mnemonicParsing="false" onAction="#backToMenuSignal" prefHeight="57.0" prefWidth="187.0" style="-fx-background-color: none;" text="Back to Menu">
