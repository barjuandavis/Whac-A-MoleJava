<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="480.0" prefWidth="640.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="480.0" fitWidth="640.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Mole_BG.jpg" />
         </image>
      </ImageView>
      <VBox prefHeight="480.0" prefWidth="640.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="640.0">
               <children>
                  <Button id="casual" fx:id="casual" mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: none;" text="Casual">
                     <HBox.margin>
                        <Insets left="75.0" right="75.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Coffee At Midnight Demo" size="31.0" />
                     </font>
                  </Button>
                  <Button id="hardCore" fx:id="hardCore" mnemonicParsing="false" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: none;" text="Hardcore">
                     <HBox.margin>
                        <Insets left="75.0" right="75.0" />
                     </HBox.margin>
                     <font>
                        <Font name="Coffee At Midnight Demo" size="31.0" />
                     </font>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="200.0" />
               </VBox.margin>
            </HBox>
            <Button id="back" fx:id="back" mnemonicParsing="false" style="-fx-background-color: none;" text="Back">
               <VBox.margin>
                  <Insets left="300.0" top="100.0" />
               </VBox.margin>
               <font>
                  <Font name="Coffee At Midnight Demo" size="19.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</fx:root>
